/**index.wxss**/
cover-view {
  line-height: 1;
  white-space: normal;
}




.container {
  background-color: #fff;
}
.navbar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 88rpx;
  padding: 0 140rpx;
  font-size: 30rpx;
}
.navbar > view {
  width: 120rpx;
  height: 42rpx;
  line-height: 42rpx;
  text-align: center;
  
}
.navbar > view > text {
  display: inline-block;
  padding-bottom: 8rpx;
  border-bottom: 4rpx solid transparent;
  color: rgba(0,0,0,0.40);
}
.navbar > .active > text {
  border-bottom-color: rgba(0,0,0,0.80);
  color: rgba(0,0,0,0.80);
}

.pannel-wrap > view {
  display: none;
}
.pannel-wrap > .active {
  display: block;
}

/* .fixed-btn {
  position: fixed;
  bottom: 0;
  left: 50%;
  transform: translateX(-32rpx);
} */
.btn-user {
  width: 64rpx;
  height: 64rpx;
  margin: 100rpx auto;
}

.canvas-wrap {
  margin-top: 160rpx;
}

.count-wrap > view,
.canvas-wrap .btn-begin{
  margin-top: 120rpx;
}

.count-wrap > view > .btn-wrap {
  margin-bottom: 40rpx;
}

.modal {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0,0,0,.2);
}
.modal-wrap {
  box-sizing: border-box;
  position: relative;
  width: 590rpx;
  height: 800rpx;
  color: #fff;
  border-radius: 28rpx;
  text-align: center;
  padding: 60rpx 56rpx;
  background-color: #f00;
}
.icon-cancle {
  position: absolute;
  top: 16rpx;
  right: 16rpx;
  width: 48rpx;
  height: 48rpx;
}
.modal-success {
  background-color: #65AD6F;
}
.modal-fail {
  background-color: #C76767;
}
.title {
  font-size: 34rpx;
  height: 48rpx;
  line-height: 48rpx;
}
.result {
  font-size: 62rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin-top: 30rpx;
}
.icon-result-wrap {
  width: 92rpx;
  height: 92rpx;
  margin: 30rpx auto 0;
}
.icon-result {
  width: 92rpx;
  height: 92rpx;
}
.result-message {
  font-size: 34rpx;
  height: 96rpx;
  line-height: 48rpx;
  margin: 80rpx auto 100rpx;
  white-space: normal;
}
.btn-cover-inner,
.btn-cover{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  background-color: transparent!important;
  color: #fff!important;
  border-color: #fff!important;
  opacity: 0;
}
.wx-button-cover-view-inner,
.wx-button-cover-view-wrapper {
  opacity: 0;
}
.btn-cover-wrap {
  position: relative;
  width: 300rpx;
  height: 88rpx;
  line-height: 88rpx;
  border: 1px solid #fff;
  border-radius: 20rpx;
  margin: 0 auto;
}
