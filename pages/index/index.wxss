/* 排名 */
.h20 {
  height: 20rpx;
  background-color: #F2F2F2;
}
.remain-time {
  font-size: 26rpx;
  color: rgba(0,0,0,.6);
  text-align: center;
  padding: 30rpx 0 40rpx;
}
.remain-time > .time-wrap {
  margin-top: 20rpx;
}
.remain-time > .time-wrap > .icon-time{
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
  line-height: 76rpx;
  font-size: 34rpx;
  color: rgba(0,0,0,0.60);
  border: 2rpx solid rgba(0,0,0,0.12);
  border-radius: 10rpx;
}
.remain-time > .time-wrap > text:not(:last-child) {
  margin-right: 10rpx;
}


.rank-list {
  padding: 0 30rpx;
}
.rank-item {
  display: flex;
  justify-content: space-between;
  padding: 26rpx 0 32rpx;
  border-bottom: 2rpx solid rgba(0,0,0,0.12);
}
.rank-item > image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
  margin: 12rpx 0;
  overflow: hidden;
}
.rank-item > .user {
  flex-grow: 1;
  padding-left: 16rpx;
}
.rank-item > .user > .name {
  font-size: 30rpx;
  line-height: 42rpx;
  color: rgba(0,0,0,0.60);
}
.rank-item > .user > view > text {
  font-size: 26rpx;
  padding-left: 40rpx;
  margin-right: 30rpx;
  background-repeat: no-repeat;
  background-size: 36rpx 36rpx;
  background-position: left center;
}
.icon-total-success {
  color: #5C9965;
  background-image: url(data:image/svg+xml;base64,);
}
.icon-total-fail {
  color: #C76767;
  background-image: url(data:image/svg+xml;base64,);
}
.icon-total {
  color: rgba(0,0,0,0.60);
  background-image: url(data:image/svg+xml;base64,);
}
.rank-item > .number {
  width: 60rpx;
  text-align: center;
}

.rank-item > .number > text {
  display: block;
  line-height: 36rpx;
  font-size: 26rpx;
  color: rgba(0,0,0,0.20);
}
.rank-item > .number > .icon-number {
  width: 44rpx;
  height: 44rpx;
  line-height: 44rpx;
  margin: 0 auto 12rpx;
  font-size: 30rpx;
  color: rgba(0,0,0,0.60);
}
.rank-list > .rank-item:nth-child(-n+3) > .number > .icon-number {
  background-color: #5C9965;
  color: #fff;
  border-radius: 10rpx;
}


/* 统计 */
.count-all-wrap {
  padding: 46rpx 0;
}
.count-all-wrap > .count-all:first-child > .count-all-item{
  padding-bottom: 40rpx;
}
.count-all {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.count-all > .count-all-item {
  width: 50%;
  text-align: center;
}
.count-all > .count-all-item:nth-child(odd) {
  border-right: 1rpx solid rgba(0,0,0,0.12);
}
.count-all-item > .desc {
  display: block;
  font-size: 26rpx;
  color: rgba(0,0,0,0.60);
}
.count-all-item > .count {
  position: relative;
  display: inline-block;
  font-size: 48rpx;
  color: rgba(0,0,0,0.60);
}
.count-all-item > .count > .subup {
  position: absolute;
  top: 0;
  right: 0;
  width: 44rpx;
  font-size: 26rpx;
  text-align: left;
  transform: translateX(50rpx)
}

.count-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 96rpx;
  line-height: 36rpx;
  padding: 30rpx 40rpx;
  font-size: 26rpx;
}
.count-title > text:first-child {
  color: rgba(0,0,0,0.60);
}
.count-title > text:last-child {
  color: rgba(0,0,0,0.40);
}

.week-canvas {
  width: 315px;
  margin: 30rpx auto;
}


.count-time {
  display: flex;
  flex-wrap: wrap;
  text-align: center;
}
.count-time > .count-time-item {
  width: 50%;
  margin-bottom: 30rpx;
}
.count-time-item > text {
  display: block;
}
.count-time-item > .count-time-percent {
  line-height: 48rpx;
  margin-bottom: 4rpx;
  font-size: 34rpx;
  color: rgba(0,0,0,0.60);
}
.count-time-item > .time-range {
  line-height: 32rpx;
  font-size: 22rpx;
  color: rgba(0,0,0,0.30);
}