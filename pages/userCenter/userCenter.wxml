<view class="container">
  <view class="head-wrap">
    <view class="user-wrap">
      <view class="avatar">
        <image mode="aspectFit" src="{{userInfo.avatarUrl}}"></image>
      </view>
      <text>{{ userInfo.nickName }}</text>
    </view>
    <view class="count-all">
      <view class="count-all-item">
        <text class="count">{{ focusCount }}</text>
        <text class="desc">累计次数</text>
      </view>
      <view class="count-all-item">
        <text class="count">{{ focusTime }}</text>
        <text class="desc">累计小时</text>
      </view>
    </view>
  </view>
  <view class="cell-list">
    <view class="cell-item icon-test">
        <navigator url="/pages/evaluating/evaluating">
          <text>自制力测试</text>
          <text>测试看真相</text>
        </navigator>
    </view>
    <view class="cell-item icon-about">
        <navigator url="/pages/evaluating/evaluating">
          <text>小程序说明</text>
          <text></text>
        </navigator>
    </view>
  </view>
</view>