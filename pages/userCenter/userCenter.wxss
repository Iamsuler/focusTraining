page {
  background-color: #F2F2F2;
}
.container  > view {background-color: #fff;}


.count-all {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.count-all > .count-all-item {
  width: 50%;
  text-align: center;
}
.count-all > .count-all-item:nth-child(odd) {
  border-right: 1rpx solid rgba(0,0,0,0.12);
}
.count-all-item > .desc {
  display: block;
  font-size: 26rpx;
  color: rgba(0,0,0,0.60);
}
.count-all-item > .count {
  position: relative;
  display: inline-block;
  font-size: 48rpx;
  line-height: 60rpx;
  color: rgba(0,0,0,0.60);
}
.count-all-item > .count > .subup {
  position: absolute;
  top: 0;
  right: 0;
  width: 44rpx;
  font-size: 26rpx;
  text-align: left;
  transform: translateX(50rpx)
}

.head-wrap {
  padding: 40rpx 0 60rpx;
  margin-bottom: 20rpx;
}
.user-wrap {
  margin-bottom: 34rpx;
  font-size: 34rpx;
  color: rgba(0,0,0,0.60);
  text-align: center;
}
.user-wrap > .avatar {
  width: 120rpx;
  height: 120rpx;
  margin: 0 auto 24rpx;
  border-radius: 60rpx;
  overflow: hidden;
}
.user-wrap > .avatar > image {
  width: 120rpx;
  height: 120rpx;
}


.icon-about {
  background-image: url(data:image/png;base64,)
}
.icon-test {
  background-image: url(data:image/png;base64,);
}
.cell-list {
  padding-left: 40rpx;
}
.cell-item {
  padding-left: 80rpx;
  background-position: left center;
  background-size: 48rpx 48rpx;
  background-repeat: no-repeat
}
.cell-list > .cell-item:not(:last-child) > navigator {
  border-bottom: 2rpx solid rgba(0,0,0,0.12);
}
.cell-item > navigator {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  padding-right: 40rpx;
}
.cell-item > navigator > text {
  font-size: 34rpx;
  color: rgba(0,0,0,0.60);
}
.cell-item > navigator > text:last-child {
  padding-right: 20rpx;
  font-size: 30rpx;
  color: rgba(0,0,0,0.30);
}