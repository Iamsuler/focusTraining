/**app.wxss**/
page {
  min-height: 100%;
}
.container {
  min-height: 100%;
  height: 100%;
}
view, text {
  box-sizing: border-box;
}

.navigator-hover {
  background-color: transparent;
  opacity: 1;
}
.btn {
  display: block;
  width: 240rpx;
  height: 88rpx;
  line-height: 84rpx;
  margin: 0 auto;
  border: 2rpx solid transparent;
  border-radius: 20rpx;
  text-align: center;
  color: #fff;
  background-color: #fff;
}

.btn-prime {
  background-color: #5C9965;
}

.btn-prime-clear {
  border-color: #5C9965;
  color: #5C9965;
}

